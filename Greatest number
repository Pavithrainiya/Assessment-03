import java.util.Scanner;
public class Main
{
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
        int a=10,b=15,c=20;
        System.out.println((a>b)?((a>c)?"a is max":"c is max"):((b>c)?"b is max":"c is max"));
    }
}

